# Production Environment Variables
# Скопируйте этот файл в .env на продакшн сервере и измените значения

# Database - PostgreSQL for production
DATABASE_URL="postgresql://ai_tools_user:your_secure_password@localhost:5432/ai_tools_prod"

# PostgreSQL Configuration
POSTGRES_DB=ai_tools_prod
POSTGRES_USER=ai_tools_user
POSTGRES_PASSWORD=your_secure_password

# Application
NODE_ENV=production
LOG_LEVEL=info

# Optional: If using external PostgreSQL
# DATABASE_URL="postgresql://username:password@your-db-host:5432/ai_tools_prod" 